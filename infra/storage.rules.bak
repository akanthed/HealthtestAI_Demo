rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /requirements/{allPaths=**} {
      allow write: if request.auth != null && request.auth.token.role in ['qa','admin'];
      allow read: if request.auth != null && request.auth.token.role in ['qa','sme','admin','auditor'];
    }
  }
}